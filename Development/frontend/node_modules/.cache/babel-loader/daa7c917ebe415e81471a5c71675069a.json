p.random(45, 100);\n\n          if (i < stars.length / 4) {\n            r = p.random(200, 255);\n            g = p.random(200, 255);\n            b = 0;\n          } else if (i >= stars.length / 4 && i < stars.length / 2) {\n            r = p.random(200, 255);\n            g = p.random(20, 40);\n            b = p.random(20, 40);\n          } else if (i >= stars.length / 2 && i < stars.length / 1.5) {\n            r = p.random(20, 40);\n            g = p.random(200, 255);\n            b = p.random(20, 40);\n          } else {\n            r = p.random(20, 40);\n            g = p.random(20, 40);\n            b = p.random(200, 255);\n          }\n\n          galaxy[i] = new GalaxyStars(p.random(width), p.random(height), p.random(1.0, 1.5), p.random(1.5, 2.5), p.color(r, g, b, a), p.random(255));\n        }\n      }\n\n      function drawGalaxyBG() {\n        for (let i = 0; i < galaxy.length; i++) {\n          galaxy[i].show(p);\n        }\n      }\n\n      p.setup = () => {\n        p.createCanvas(width, height).parent('p5Canvas');\n        p.background(0);\n        makeBGStars();\n      };\n      p.draw = () => {\n        p.background(10);\n        drawGalaxyBG();\n        // p.text('hello world!', 50, 100);\n      };\n    };\n\n    onMounted(() => {\n      new p5(sketch);\n    });\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n@import '@/sass/Unique/_bg';\n</style>\n"],"sourceRoot":""}]}